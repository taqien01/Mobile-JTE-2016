<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.ac.unsyiah.elektro.mobile.tugas05_spreadsheet.MainActivityFragment"
    tools:showIn="@layout/activity_main">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/a11"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:width="80dp"
        android:text="0"
        android:textAlignment="center"
        android:selectAllOnFocus="true"
        android:textIsSelectable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/a12"
        android:width="80dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/a11"
        android:layout_toEndOf="@+id/a11"
        android:text="0"
        android:textAlignment="center"
        android:selectAllOnFocus="true"
        android:textIsSelectable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/a21"
        android:width="80dp"
        android:layout_below="@+id/a11"
        android:layout_toLeftOf="@+id/a12"
        android:layout_toStartOf="@+id/a12"
        android:text="0"
        android:textAlignment="center"
        android:selectAllOnFocus="true"
        android:textIsSelectable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/a22"
        android:width="80dp"
        android:layout_below="@+id/a12"
        android:layout_alignLeft="@+id/a12"
        android:layout_alignStart="@+id/a12"
        android:text="0"
        android:textAlignment="center"
        android:selectAllOnFocus="true"
        android:textIsSelectable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/a13"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/a12"
        android:layout_toEndOf="@+id/a12"
        android:width="80dp"
        android:textAlignment="center"
        android:text="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/a23"
        android:width="80dp"
        android:layout_below="@+id/a12"
        android:layout_toRightOf="@+id/a22"
        android:layout_toEndOf="@+id/a22"
        android:textAlignment="center"
        android:text="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/a31"
        android:width="80dp"
        android:textAlignment="center"
        android:text="0"
        android:layout_below="@+id/a21"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/a32"
        android:width="80dp"
        android:textAlignment="center"
        android:layout_alignTop="@+id/a31"
        android:layout_alignLeft="@+id/a22"
        android:layout_alignStart="@+id/a22"
        android:text="0" />

    <!--
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/spnOperasi"
        android:layout_below="@+id/a22"
        android:layout_toRightOf="@+id/a22"
        android:layout_toEndOf="@+id/a22"
        android:entries="@array/operasi"
        android:onClick="onClickSamaDengan" />
    -->


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="="
        android:id="@+id/button"
        android:layout_below="@+id/a22"
        android:layout_toRightOf="@+id/a32"
        android:layout_toEndOf="@+id/a32"
        android:onClick="onClickSamaDengan" />

</RelativeLayout>
